  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cucumber": "^3",
    "cucumber-html-reporter": "^3.0.4"
  },
